set clipboard=unnamed

nmap H :obcommand<space>workspace:previous-tab<CR>
nmap L :obcommand<space>workspace:next-tab<CR>

nmap sl :obcommand<space>editor:focus-right<CR>
nmap sh :obcommand<space>editor:focus-left<CR>
nmap sk :obcommand<space>editor:focus-top<CR>
nmap sj :obcommand<space>editor:focus-bottom<CR>

nmap za :obcommand<space>editor:toggle-fold<CR>


